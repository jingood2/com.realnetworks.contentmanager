{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "morgan": {
      "params": "combined"
    },
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {
    "body-parser#json": {},
    "body-parser#urlencoded": {
      "params": {
        "extended": true
      }
    },
    "body-parser#raw": {}
  },
  "routes": {},
  "files": {
    "loopback#static" : [{
      "name" : "app",
      "paths" : ["/"],
      "params" : "$!../client"
    },
      {
        "name" : "ments",
        "paths" : ["/ments"],
        "params" : "/home/strong-pm/storage/ments"
      },
      {
        "name" : "records",
        "paths" : ["/records"],
        "params" : "/home/strong-pm/storage/records"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "errorhandler": {}
  }
}
